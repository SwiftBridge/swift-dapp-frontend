# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Base Mainnet RPC URL
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Contract addresses (JSON string)
NEXT_PUBLIC_CONTRACT_ADDRESSES={"socialMessaging":"0x...","batchMessaging":"0x...","batchTransactions":"0x...","userManagement":"0x...","messageStorage":"0x...","notifications":"0x...","governance":"0x...","utilities":"0x..."}

# IPFS Configuration
NEXT_PUBLIC_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# Push Protocol (for notifications)
NEXT_PUBLIC_PUSH_PROTOCOL_API_KEY=your_push_protocol_key_here
